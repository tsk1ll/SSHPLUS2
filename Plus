cd /bin/ && rm -f backup_mail.sh*
cd /bin/ && rm -f celular.sh*
cd /bin/ && rm -f beta-v2ray.sh*
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/v2ray clean > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/multi > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/blocksite > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/websocket.sh > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/shutdown -r 16:00 > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/multi.sh > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/swapmemory > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/tweaker > /dev/null 2>&1
