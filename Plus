cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/dns-netflix.sh > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/blockuser > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Sistema/script/botpro > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/Autobackup > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/backup_mail.sh > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/celular.sh > /dev/null 2>&1
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/beta-v2ray.sh > /dev/null 2>&1
cd /bin/ && chmod +x beta-v2ray.sh
cd /bin/ && chmod +x blocksite
cd /bin/ && chmod +x multi
cd /bin/ && chmod +x swapmemory
cd /bin/ && chmod +x shutdown -r 00:00
cd /bin/ && chmod +x websocket.sh
cd /bin/ && chmod +x tweaker
cd /bin/ && chmod +x webmin.sh
cd /bin/ && chmod +x prnet.sh
cd /bin/ && chmod +x dns-netflix.sh
cd /bin/ && chmod +x blockuser
cd /bin/ && chmod +x v2ray clean
cd /bin/ && chmod +x botpro
cd /bin/ && chmod +x Autobackup
cd /bin/ && chmod +x backup_mail.sh
cd /bin/ && chmod +x celular.sh
cd /bin/ && rm -f dns-server*
cd /bin/ && rm -f remove-slow*
cd /bin/ && rm -f restartdns*
cd /bin/ && rm -f slowdns-drop*
cd /bin/ && rm -f slowdns-info*
cd /bin/ && rm -f slowdns-socks*
cd /bin/ && rm -f slowdns-ssh*
cd /bin/ && rm -f slowdns-ssl*
cd /bin/ && rm -f startdns*
cd /bin/ && rm -f stopdns*
rm -rf /bin/slowdns*
mkdir /bin/slowdns*
rm -f /bin/slowdns*
cd /bin/ && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/slowdns > /dev/null 2>&1
cd /bin/slowdns && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/dns-server > /dev/null  2>&1
cd /bin/slowdns && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/remove-slow > /dev/null 2>&1
cd /bin/slowdns && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/restartdns > /dev/null 2>&1
cd /bin/slowdns && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/slowdns-drop > /dev/null 2>&1
cd /bin/slowdns && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/slowdns-info > /dev/null 2>&1
cd /bin/slowdns && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/slowdns-socks > /dev/null 2>&1
cd /bin/slowdns && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/slowdns-ssh > /dev/null 2>&1
cd /bin/slowdns && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/slowdns-ssl > /dev/null 2>&1
cd /bin/slowdns && wget https://github.com/ogenio/SSHPLUS/raw/master/Modulos/startdns > /dev/null 2>&1
